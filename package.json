{
  "name": "typist",
  "version": "0.6.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "https://typistapp.netlify.com",
  "scripts": {
    "start": "react-scripts start",
    "start:electron": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "lint": "eslint ./src/",
    "deploy:firebase": "node scripts/write-firebase-file.js",
    "prebuild": "run-s lint deploy:firebase",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postversion": "run-s changelog",
    "changelog": "auto-changelog -p -t .changelog.hbs"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "firebase": "^5.5.6",
    "lodash.samplesize": "^4.2.0",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-avatar": "^3.4.6",
    "react-dom": "^16.6.0",
    "react-ga": "^2.5.3",
    "react-google-button": "^0.5.3",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-timeago": "^4.1.9",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "auto-changelog": "^1.8.1",
    "concurrently": "^4.0.1",
    "electron": "^3.0.6",
    "eslint-config-react-app": "^3.0.4",
    "eslint-plugin-jest": "^21.25.1",
    "npm-run-all": "^4.1.3",
    "react-test-renderer": "^16.6.0",
    "wait-on": "^3.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vimark1/typist.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vimark1/typist/issues"
  }
}
