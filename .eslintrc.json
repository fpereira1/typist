{
  "extends": ["eslint:recommended", "react-app", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules":{
    "no-console": "off",
    "no-use-before-define": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
