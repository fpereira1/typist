{
  "extends": ["eslint:recommended", "react-app", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules":{
    "no-console": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }]
  }
}
